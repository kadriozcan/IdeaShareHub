@model List<Entity.About>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>About Data List</h2>
<table class="table table-bordered">
    <tr>
        <th>ID</th>
        <th>Title</th>
        <th></th>
    </tr>
    @foreach (Entity.About about in Model)
    {
        <tr>
            <td>@about.Id</td>
            <td>titledsfasddddddd</td>
            <td><a href="#" class="btn btn-success mx-5">Activate</a><a href="#" class="btn btn-danger">Deactivate</a></td>
        </tr>
    }
</table>

@Html.Action("AboutPartial", "About")

